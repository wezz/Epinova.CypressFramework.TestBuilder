# Uncomment if necessary howusing System.Threading;
using System.Threading.Tasks;
using AutoMoq;
using FluentAssertions;
using JourPlanner.Application.Feature.Statistics;
using JourPlanner.Application.Feature.Statistics.SlotStatisticsQuery;
using JourPlanner.Application.Legacy.Butler;
using JourPlanner.Command.Filter;
using JourPlanner.DomainModel;
using Moq;
using NUnit.Framework;

namespace Jour.Application.Tests
{
    [TestFixture]
    public class SlotStatisticsQueryHandlerTests
    {
        private AutoMoqer _autoMoqer;
        private SlotStatisticsQueryHandler _handler;

        [SetUp]
        public void Setup()
        {
            _autoMoqer = new AutoMoqer();
            _handler = _autoMoqer.Create<SlotStatisticsQueryHandler>();
        }

        [Test]
        public async Task GetCalendarMonthData_should_return_slot_statistics()
        {
            //Arrange
            var loginUserDomainModel = new LoginUserDomainModel
            {
                Id = 1,
                Calendar = new Infrastructure.Data.Models.Calendar { Id = 1 },
                CompanyId = 1
            };
            var statisticFilter = new StatisticFilter();
            var slotStatistics = new SlotStatistics();
            _autoMoqer.GetMock<IPlanningButler>()
                .Setup(x => x.GetSlotStatistics(It.IsAny<StatisticFilter>()))
                .Returns(slotStatistics);

            //Act
            var result = await _handler.Handle(new SlotStatisticsQuery(statisticFilter, loginUserDomainModel), CancellationToken.None);

            //Assert
            result.Should().BeOfType<SlotStatistics>();
        }
    }
}
